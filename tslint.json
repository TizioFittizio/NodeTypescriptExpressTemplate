{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-sonarts"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "no-console": [false],
        "no-var-requires": false,
        "arrow-parens": false,
        "ordered-imports": false,
        "one-line": false,
        "trailing-comma": false,
        "object-literal-sort-keys": false,
        "only-arrow-functions": false
    },
    "rulesDirectory": []
}